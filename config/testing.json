{
   "db": {
      "driver": "SQLITE",
      "filepath": "database.sqlite",
      "name": "settleplate",
      "table": "SETTLEPLATE"
   },
   "regex": {
      "user": [
         "^user:(?P<user>.+)$"
      ],
      "batch": [
         "^(?P<batch>[A-Za-z]{3,5}\\d{7})$"
      ],
      "location": [
         "^loc:(?P<location>.+)$"
      ],
      "settleplate": [
         "^(?P<serial>\\d+(?P<lot>\\d{11})(?P<year>\\d{2})(?P<month>\\d{2})(?P<day>\\d{2}))$",
         "^(?P<serial>\\d+\\s?(?P<lot>\\d{10})(?P<year>\\d{4})(?P<month>\\d{2})(?P<day>\\d{2})\\w+)$"
      ]
   },
   "general": {
      "adminpwd": "admin",
      "user_min": 4,
      "user_max": 8,
      "timeout": 300,
      "mountpoint": "/mnt/data"
   },
   "users": {
      "admin": "admin"
   },
   "camera": {
      "default": {
         "crop": [650, 16, 3008, 3008],
         "image": true,
         "hflip": false,
         "vflip": true,
         "rotation": "cw",
         "light": "none",
         "color": [0,0,0],
         "exposure": 50000,
         "wb": [2.3,2,3],
         "autocrop" : "none",
         "mask" : false,
         "drawmask" : false
      },
      "ring": {
         "light": "ring",
         "color": [255,78,36],
         "exposure": 100000,
         "wb": [1.8,1.8],
         "mask": true
      },
      "top": {
         "light": "top",
         "color": [255,76,25],
         "exposure": 33000,
         "wb": [1.8,1.8]
      },
      "debug": {
         "drawmask": true
      }
   },
   "autocrop": {
      "settleplate": {
         "c_x": 0.506,
         "c_y": 0.5,
         "c_r": 0.435,
         "mask": true,
         "crop": false
      },
      "plate": {
         "autolevel": false
      }
   }
}